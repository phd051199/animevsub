{
	"appId": "git.shin.animevsub",
	"appName": "AnimeVsub",
	"plugins": {
		"CapacitorCookies": {
			"enabled": true
		},
		"CapacitorHttp": {
			"enabled": true
		}
	},
	"webDir": "www",
	"server": {
		"androidScheme": "https"
	},
	"packageClassList": [
		"FirebaseAnalytics",
		"AppPlugin",
		"CAPBrowserPlugin",
		"DevicePlugin",
		"FilesystemPlugin",
		"HapticsPlugin",
		"PreferencesPlugin",
		"SharePlugin",
		"StatusBarPlugin",
		"ScreenOrientationPlugin",
		"NavigationBarPlugin",
		"CDVPlugin"
	]
}
